<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo4.window1Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
   <Pane prefHeight="200.0" prefWidth="200.0">
      <children>
         <Pane prefHeight="200.0" prefWidth="200.0">
            <children>
               <Button fx:id="btNok" layoutX="48.0" layoutY="25.0" mnemonicParsing="false" onAction="#changeWindow" onMouseClicked="#changeWindow" prefHeight="37.0" prefWidth="104.0" text="Регистрация" />
               <Button fx:id="bt" layoutX="48.0" layoutY="100.0" mnemonicParsing="false" onAction="#winChange" prefHeight="37.0" prefWidth="104.0" text="Авторизация" />
            </children></Pane>
      </children>
   </Pane>
</VBox>
